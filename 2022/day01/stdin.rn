class Stdin(self) {

    self.EOF = -1u8

    func readLine(self, var eof) {
        line = ""
        do {
            c = readByte()
        } while c != '\n' && c != self.EOF {
            line.append(c)
        }
        eof = c == self.EOF
        return line
    }

    iterator lines(self) {
        eof = false
        do {
            line = self.readLine(eof)
        } while !eof {
            yield line
        }
    }
}
